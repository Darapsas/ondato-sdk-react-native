{
  "name": "ondato-sdk-react-native",
  "version": "0.1.4",
  "description": "Ondato SDK for React Native",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "assets",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "android",
    "ios",
    "cpp",
    "idv-sdk-reactnative.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build"
  ],
  "scripts": {
    "prepack": "bob build",
    "prepare": "bob build",
    "typescript": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "release": "release-it",
    "pods": "cd example && pod-install --quiet"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/ondato/ondato-sdk-react-native",
  "author": "Ondato",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.3",
    "@reduxjs/toolkit": "^1.8.6",
    "@tanstack/react-query": "^4.13.5",
    "axios": "^0.27.2",
    "axios-logger": "^2.6.2",
    "hex-to-rgba": "^2.0.1",
    "i18next": "^22.0.4",
    "lottie-ios": "3.4.0",
    "lottie-react-native": "^5.1.4",
    "react-i18next": "^12.0.0",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-record-screen": "^0.4.0",
    "react-native-toast-message": "^2.1.6",
    "react-redux": "^8.0.4",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@react-native-community/eslint-config": "^3.0.2",
    "@release-it/conventional-changelog": "^5.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/react": "^18.0.28",
    "@types/react-native": "^0.71.3",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "react-native-safe-area-context": "4.x",
    "react-native-fs": "2.x",
    "react-native-vision-camera": "2.x",
    "react-native-svg": "^13.8.0",
    "react-native-svg-transformer": "^1.0.0",
    "@typescript-eslint/parser": "^5.37.0",
    "react-native-facetec": "git+https://github.com/ondato/ondato-facetec-react-native.git",
    "commitlint": "^17.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^6.0.0",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "17.0.2",
    "react-native": "0.68.6",
    "react-native-builder-bob": "^0.20.3",
    "release-it": "^15.0.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react-native-safe-area-context": "4.x",
    "react-native-gesture-handler": "2.x",
    "react-native-reanimated": "2.x",
    "react-native-screens": "3.x",
    "react-native-fs": "2.x",
    "react-native-svg": "13.x",
    "react-native-svg-transformer": "1.x",
    "react-native-vision-camera": "2.x",
    "react-native-facetec": "git+https://github.com/ondato/ondato-facetec-react-native.git"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
